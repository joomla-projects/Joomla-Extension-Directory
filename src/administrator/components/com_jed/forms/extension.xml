<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="Jed\Component\Jed\Administrator\Field">
    <fieldset>
        <field name="id" type="text" readonly="true" class="readonly" default="0"/>
        <field name="title" filter="safehtml" type="text" label="JGLOBAL_TITLE"
               hint="COM_JED_FORM_LBL_EXTENSION_TITLE"/>
        <field name="alias" type="text" class="inputbox" generator="title" label="JFIELD_ALIAS_LABEL" hint="JALIAS"/>

    </fieldset>
    <fieldset name="info" label="Info">


        <field
                name="version"
                type="text"
                label="COM_JED_FORM_LBL_EXTENSION_VERSION"
                class="input-mini"
        />
		<field name="primary_category_id" extension="com_jed" type="jedcategory" published="1" multiple="false"
			   label="COM_JED_FORM_LBL_EXTENSION_PRIMARY_CATEGORY_ID"
			   hint="COM_JED_FORM_LBL_EXTENSION_PRIMARY_CATEGORY_ID"/>

        <field
                name="includes"
                type="checkboxes"
                label="COM_JED_EXTENSIONS_EXTENSION_TYPES"
        >
            <option value="com">COM_JED_EXTENSIONS_COMPONENT</option>
            <option value="mod">COM_JED_EXTENSIONS_MODULE</option>
            <option value="plugin">COM_JED_EXTENSIONS_PLUGIN</option>
            <option value="specific">COM_JED_EXTENSIONS_SPECIFIC</option>
        </field>
        <field
                name="joomla_versions"
                type="checkboxes"
                label="COM_JED_FORM_LBL_EXTENSION_JOOMLA_VERSIONS"
        >
            <option value="30">3.0</option>
            <option value="40">4.0</option>
            <option value="41">4.1</option>
            <option value="42">4.2</option>
            <option value="42">4.3</option>
            <option value="42">4.4</option>
            <option value="50">5.0</option>
            <option value="60">6.0</option>
        </field>

        <field
                name="uses_updater"
                type="radio"
                label="COM_JED_FORM_LBL_EXTENSION_USES_UPDATER"
                class="btn-group btn-group-yesno"
                default="0"
                filter="integer"
        >
            <option value="0">JNO</option>
            <option value="1">JYES</option>
        </field>
        <field name="gpl_license_type"
               type="list"
               default=""
               class="input-xlarge"
               label="COM_JED_FORM_LBL_EXTENSION_GPL_LICENSE_TYPE">
            <option value="GPLv2 or later">COM_JED_EXTENSION_GPL_LICENSE_TYPE_OPTION_GPL2</option>
            <option value="AGPL">COM_JED_EXTENSION_GPL_LICENSE_TYPE_OPTION_AGPL</option>
            <option value="LGPL">COM_JED_EXTENSION_GPL_LICENSE_TYPE_OPTION_LGPL</option>
            <validations>
                <validation name="required"
                            validatelevel="3" />
            </validations>

        </field>
        <field
                name="nonGplCssJs"
                type="radio"
                label="COM_JED_EXTENSIONS_USES_NON_GPL"
                class="btn-group btn-group-yesno"
                default="0"
                filter="integer"
        >
            <option value="0">JNO</option>
            <option value="1">JYES</option>
        </field>

        <field
                name="uses_third_party"
                type="radio"
                label="COM_JED_FORM_LBL_EXTENSION_USES_THIRD_PARTY"
                class="btn-group btn-group-yesno"
                default="0"
                filter="integer"
        >
            <option value="0">JNO</option>
            <option value="1">JYES</option>
        </field>
        <field
                name="requires_registration"
                type="radio"
                label="COM_JED_FORM_LBL_EXTENSION_REQUIRES_REGISTRATION"
                class="btn-group btn-group-yesno"
                default="0"
                filter="integer"
        >
            <option value="0">JNO</option>
            <option value="1">JYES</option>
        </field>
        <field
                name="logo"
                type="media"
                label="COM_JED_EXTENSIONS_LOGO"
                class="input-xxlarge"
                preview="true"
                directory=""
        />
        <field name="video"
               type="text"
               class="span12"
               label="COM_JED_FORM_LBL_EXTENSION_VIDEO" />
        <field
                name="developer_company"
                type="text"
                label="COM_JED_VEL_GENERAL_FIELD_DEVELOPER_NAME_LABEL"
                class="input-mini"
        />
        <field
            name="developer_email"
            type="text"
            label="Developer Email"
            class="input-mini"
    /><field
            name="developer_username"
            type="text"
            label="Developer User"
            class="input-mini"
    />
    </fieldset>
	</form>
