<?xml version="1.0" encoding="utf-8"?>
<form enctype="multipart/form-data" method="post" addfieldprefix="Jed\Component\Jed\Site\Field">
    <fieldset>
        <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
        <field name="title" filter="safehtml" type="text" label="COM_JED_EXTENSION_FIELD_NAME_LABEL"
               description="COM_JED_EXTENSION_FIELD_NAME_DESCR" required="true" hint=""/>
        <field name="alias" type="text" class="inputbox" generator="title" label="JFIELD_ALIAS_LABEL"
               description="JFIELD_ALIAS_DESC" hint="Do not enter anything here, it is autogenerated"/>
        <field name="version" filter="safehtml" type="text" label="COM_JED_EXTENSION_FIELD_VERSION_LABEL"
               description="COM_JED_EXTENSION_FIELD_VERSION_DESCR" required="true" hint=""/>
        <field
                name="includes"
                type="checkboxes"
                label="COM_JED_EXTENSION_FIELD_EXTENSION_TYPES_LABEL"
        >
            <option value="com">COM_JED_EXTENSION_FIELD_EXTENSION_TYPES_OPTION_COMPONENT</option>
            <option value="mod">COM_JED_EXTENSION_FIELD_EXTENSION_TYPES_OPTION_MODULE</option>
            <option value="plugin">COM_JED_EXTENSION_FIELD_EXTENSION_TYPES_OPTION_PLUGIN</option>
            <option value="specific">COM_JED_EXTENSION_FIELD_EXTENSION_TYPES_OPTION_SPECIFIC</option>
        </field>
        <field
                name="joomla_versions"
                type="joomlaversion"
                label="COM_JED_EXTENSION_FIELD_JOOMLA_VERSIONS_LABEL"
                multiple="true"
        />
        <field name="gpl_license_type"
               type="list"
               default=""
               class="input-xlarge"
               label="COM_JED_FORM_LBL_EXTENSION_GPL_LICENSE_TYPE">
            <option value="GPLv2 or later">COM_JED_EXTENSION_FIELD_GPL_LICENSE_TYPE_OPTION_GPL2</option>
            <option value="AGPL">COM_JED_EXTENSION_FIELD_GPL_LICENSE_TYPE_OPTION_AGPL</option>
            <option value="LGPL">COM_JED_EXTENSION_FIELD_GPL_LICENSE_TYPE_OPTION_LGPL</option>
            <validations>
                <validation name="required"
                            validatelevel="3" />
            </validations>

        </field>
        <field name="uses_updater" default="0" type="radio" class="btn-group btn-group-yesno" label="COM_JED_EXTENSION_FIELD_USES_UPDATER_LABEL">
            <option value="1">JYES</option>
            <option value="0">JNO</option>
        </field>
        <field
                name="uses_third_party"
                type="radio"
                label="COM_JED_EXTENSION_FIELD_USES_THIRD_PARTY_LABEL"
                description="COM_JED_EXTENSION_FIELD_USES_THIRD_PARTY_DESCR"
                class="btn-group btn-group-yesno"
                default="0"
                filter="integer"
        >
            <option value="0">JNO</option>
            <option value="1">JYES</option>
        </field>
        <field
                name="requires_registration"
                type="radio" default="0"
                filter="integer"
               label="COM_JED_FORM_LBL_EXTENSION_REQUIRES_REGISTRATION"
               class="btn-group btn-group-yesno">
            <option value="0">JNO</option>
            <option value="1">JYES</option>
        </field>
        <field name="video" type="url" label="COM_JED_EXTENSION_FIELD_VIDEO_LABEL"
               description="COM_JED_EXTENSION_FIELD_VIDEO_DESCR" hint="" validate="url" default="https://"/>
        <field name="logo"  type="file" class="file" label="COM_JED_FORM_LBL_EXTENSION_LOGO"
               description="COM_JED_FORM_DESC_EXTENSION_LOGO" hint="COM_JED_FORM_LBL_EXTENSION_LOGO"/>
        <field name="popular" type="checkbox" checked="0" label="COM_JED_FORM_LBL_EXTENSION_POPULAR"
               description="COM_JED_FORM_DESC_EXTENSION_POPULAR" hint="COM_JED_FORM_LBL_EXTENSION_POPULAR"/>
        <field name="jed_internal_note" filter="safehtml" type="textarea"
               label="COM_JED_FORM_LBL_EXTENSION_JED_INTERNAL_NOTE"
               description="COM_JED_FORM_DESC_EXTENSION_JED_INTERNAL_NOTE"
               hint="COM_JED_FORM_LBL_EXTENSION_JED_INTERNAL_NOTE"/>
        <field name="can_update" type="checkbox" checked="0" label="COM_JED_FORM_LBL_EXTENSION_CAN_UPDATE"
               description="COM_JED_FORM_DESC_EXTENSION_CAN_UPDATE" hint="COM_JED_FORM_LBL_EXTENSION_CAN_UPDATE"/>
        <field name="approved" type="checkbox" checked="0" label="COM_JED_FORM_LBL_EXTENSION_APPROVED"
               description="COM_JED_FORM_DESC_EXTENSION_APPROVED" hint="COM_JED_FORM_LBL_EXTENSION_APPROVED"/>
        <field name="approved_notes" filter="safehtml" type="text" label="COM_JED_FORM_LBL_EXTENSION_APPROVED_NOTES"
               description="COM_JED_FORM_DESC_EXTENSION_APPROVED_NOTES"
               hint="COM_JED_FORM_LBL_EXTENSION_APPROVED_NOTES"/>
        <field name="approved_reason" filter="safehtml" type="text" label="COM_JED_FORM_LBL_EXTENSION_APPROVED_REASON"
               description="COM_JED_FORM_DESC_EXTENSION_APPROVED_REASON"
               hint="COM_JED_FORM_LBL_EXTENSION_APPROVED_REASON"/>
        <field name="approved_time" type="calendar" class="inputbox" format="%Y-%m-%d %H:%M:%S" showtime="true"
               translateformat="true" label="COM_JED_FORM_LBL_EXTENSION_APPROVED_TIME"
               description="COM_JED_FORM_DESC_EXTENSION_APPROVED_TIME" hint="COM_JED_FORM_LBL_EXTENSION_APPROVED_TIME"/>
        <field name="jed_checked" type="checkbox" checked="0" label="COM_JED_FORM_LBL_EXTENSION_JED_CHECKED"
               description="COM_JED_FORM_DESC_EXTENSION_JED_CHECKED" hint="COM_JED_FORM_LBL_EXTENSION_JED_CHECKED"/>
        <field name="modified_on" type="timeupdated" label="COM_JED_FORM_LBL_EXTENSION_MODIFIED_ON"
               description="COM_JED_FORM_DESC_EXTENSION_MODIFIED_ON" hint="COM_JED_FORM_LBL_EXTENSION_MODIFIED_ON"/>
        <field name="primary_category_id" extension="com_jed" type="jedcategory" published="1" multiple="false"
               label="COM_JED_FORM_LBL_EXTENSION_PRIMARY_CATEGORY_ID"
               description="COM_JED_FORM_DESC_EXTENSION_PRIMARY_CATEGORY_ID"
               hint="COM_JED_FORM_LBL_EXTENSION_PRIMARY_CATEGORY_ID"/>
        <field name="published" type="checkbox" checked="0" label="JPUBLISHED"
               description="COM_JED_FORM_DESC_EXTENSION_PUBLISHED" hint="JPUBLISHED"/>
        <field name="published_notes" filter="safehtml" type="text" label="COM_JED_FORM_LBL_EXTENSION_PUBLISHED_NOTES"
               description="COM_JED_FORM_DESC_EXTENSION_PUBLISHED_NOTES" hint="JPUBLISHED_NOTES"/>
        <field name="published_reason" filter="safehtml" type="text" label="COM_JED_FORM_LBL_EXTENSION_PUBLISHED_REASON"
               description="COM_JED_FORM_DESC_EXTENSION_PUBLISHED_REASON" hint="JPUBLISHED_REASON"/>
        <field name="second_contact_email" type="text" label="COM_JED_FORM_LBL_EXTENSION_SECOND_CONTACT_EMAIL"
               description="COM_JED_FORM_DESC_EXTENSION_SECOND_CONTACT_EMAIL"
               hint="COM_JED_FORM_LBL_EXTENSION_SECOND_CONTACT_EMAIL"/>
        <field name="tags"  type="tags" label="JTAG" published="1"  multiple="true" mode="ajax"
               description="JTAG_FIELD_SELECT_DESC"
               hint=""/>
        <field name="update_url" validate="url" type="url" label="COM_JED_FORM_LBL_EXTENSIONVARIEDDATUM_UPDATE_URL"
               description="COM_JED_FORM_DESC_EXTENSIONVARIEDDATUM_UPDATE_URL"
               hint="COM_JED_FORM_LBL_EXTENSIONVARIEDDATUM_UPDATE_URL"  default="https://"/>
        <field name="state" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1"
               default="1">
            <option value="1">JPUBLISHED</option>
            <option value="0">JUNPUBLISHED</option>
            <option value="2">JARCHIVED</option>
            <option value="-2">JTRASHED</option>
        </field>
        <field name="checked_out" type="hidden" filter="unset"/>
        <field name="checked_out_time" type="hidden" filter="unset"/>
        <field name="created_by" type="createdby" label="JGLOBAL_FIELD_CREATED_BY_LABEL"
               description="JGLOBAL_FIELD_CREATED_BY_DESC" hint="COM_JED_FORM_LBL_EXTENSIONVARIEDDATUM_CREATED_BY"/>
        <field name="created_on" type="timecreated" label="COM_JED_GENERAL_FIELD_CREATED_ON_LABEL"
               description="COM_JED_FORM_DESC_EXTENSION_CREATED_ON" hint="COM_JED_GENERAL_FIELD_CREATED_ON_LABEL"/>
    </fieldset>
</form>
